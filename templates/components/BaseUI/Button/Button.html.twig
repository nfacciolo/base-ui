<{{ this.tag }}
    {% if this.type %}
        type="{{ this.type }}"
    {% endif %}
    {% for attr, value in this.ariaAttributes %}
        {{ attr }}="{{ value }}"
    {% endfor %}
    {% for attr, value in this.dataAttributes %}
        {{ attr }}="{{ value }}"
    {% endfor %}
    {% if this.shouldUseNativeDisabled %}
        disabled
    {% endif %}
    {{ attributes }}
>
    {% block content %}{% endblock %}
</{{ this.tag }}>
